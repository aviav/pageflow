<% if page.template.present? %>
  <% props = %Q'{ "resolverSeed": #{@_pageflow_react_entry_seed ||= entry_json_seed(@entry)}, "pageId": #{page.perma_id}, "pageType": "#{page.template}" }' %>
  <% Pageflow.config.instrument.call('ssr.pageflow') do %>
    <% concat React::ServerRendering.render(
       'pageflow.react.ServerSidePage',
       props,
       true) %>
  <% end %>
<% end %>
