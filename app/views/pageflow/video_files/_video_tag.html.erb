<%= content_tag :video, options do %>
  <% if video_file %>
    <% video_file_sources(video_file, url_options).map do |v| %>
        <%= tag(:source, src: v[:src], type: v[:type], data: {high_src: v[:high_src]}) %>
    <% end %>
    <%= tag(:track,
            src: 'http://de.codevise.pageflow.development.out.s3-website-eu-west-1.amazonaws.com/v1/tf/video_files/000/000/054/upc-video-subtitles-en.vtt',
            srclang: 'en',
            kind: 'subtitles',
            label: 'pageflow-test',
            default: true) %>
  <% end %>
<% end %>
